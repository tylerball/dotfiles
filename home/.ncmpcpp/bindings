def_key "d"
  delete_playlist_items

def_key "D"
  select_album
  delete_playlist_items

def_key "g"
  move_home

def_key "G"
  move_end

def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "{"
  scroll_up_album

def_key "}"
  scroll_down_album

def_key "l"
  next_column

def_key "h"
  previous_column

def_key "ctrl_d"
  page_down

def_key "ctrl_u"
  page_up
